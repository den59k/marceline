  "fields": [
    {
      "fieldId": "type",
      "name": "Тип поля",
      "format": "select",
      "enum": [
        {
          "id": "creator",
          "title": "creator"
        },
        {
          "id": "designer",
          "title": "designer"
        },
        {
          "id": "reader",
          "title": "reader"
        },
        {
          "id": "manager",
          "title": "manager"
        }
      ]
    },
    {
      "children": [
        {
          "fieldId": "avatar",
          "name": "Аватар",
          "format": "file",
          "isCustom": true,
          "fileIdField": "avatarId"
        },
        {
          "fieldId": "files",
          "name": "Ещё фотографии",
          "format": "files-group",
          "isCustom": true,
          "fileIdField": "fileIds"
        }
      ]
    },
    {
      "children": [
        {
          "fieldId": "email",
          "name": "email",
          "format": "input"
        },
        {
          "fieldId": "password",
          "name": "password",
          "format": "password",
          "modifiers": [
            "hashPassword"
          ]
        }
      ]
    },
    {
      "fieldId": "features",
      "name": "Особенности пользователя",
      "format": "listInput",
      "placeholder": "Введите значение Feature"
    },
    {
      "fieldId": "deleted",
      "name": "Пользователь удален",
      "format": "checkbox"
    },
    {
      "fieldId": "location",
      "name": "location",
      "format": "geo",
      "initialLatLng": "58.009, 56.227",
      "placeholder": ""
    }
  ],
  "bodyModifiers": []
}